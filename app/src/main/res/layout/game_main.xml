    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TabWidget
            android:id="@android:id/tabs"
            android:layout_width="75dp"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical">

                <Button
                    android:id="@+id/buttonParam"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:accessibilityTraversalBefore="@+id/buttonObjets"
                    android:rotation="90"
                    android:text="paramètres"
                    android:textAlignment="center" />

                <Button
                    android:id="@+id/buttonperso"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:rotation="90"
                    android:text="personnage" />

                <Button
                    android:id="@+id/buttonObjets"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:rotation="90"
                    android:text="Equipement" />

                <Button
                    android:id="@+id/buttonContexte"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:rotation="90"
                    android:text="Contexte" />
            </LinearLayout>

